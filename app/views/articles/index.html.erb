<h1>Listing all articles</h1>
<table>
	<tr>
		<th>Title</th>
		<th>Text</th>
		<th></th>
	</tr>
	<% @articles.each do |article| %>
		<tr>
			<td><%= article.title %></td>
			<td><%= article.text %></td>
			<td><%= link_to "show", article_path(article.id) %></td>
		</tr>

	<% end %>
</table
<%= link_to "home",root_path %>